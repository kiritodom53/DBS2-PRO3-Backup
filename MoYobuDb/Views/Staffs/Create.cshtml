@model MoYobuDb.Models.Tables.Staff

@{
    ViewData["Title"] = "Vytvořit staffs";
    Layout = "_Layout_FORMS";
}

<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="name" class="control-label"></label>
        <input asp-for="name" class="form-control"/>
        <span asp-validation-for="name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="nameNative" class="control-label"></label>
        <input asp-for="nameNative" class="form-control"/>
        <span asp-validation-for="nameNative" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="surname" class="control-label"></label>
        <input asp-for="surname" class="form-control"/>
        <span asp-validation-for="surname" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="surnameNative" class="control-label"></label>
        <input asp-for="surnameNative" class="form-control"/>
        <span asp-validation-for="surnameNative" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-default"/>
    </div>
</form>


<div>
    <a asp-controller="Administration" asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}